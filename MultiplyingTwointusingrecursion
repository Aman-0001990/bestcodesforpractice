package recursion;

public class multiplication {
	public static int multiplyingTwointeger(int n,int m) {
		if( n== 0 || m==0) {
			return 0;
		}
		return m+multiplyingTwointeger(m,n-1);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     int n=7;
     int m=68;
     int ans=multiplyingTwointeger(n,m);
     System.out.print(ans);
	}

}
